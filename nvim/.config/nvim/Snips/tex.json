{
    "Environment": {
      "prefix": "beg",
      "body": [
        "\\begin{$1}",
        "\t${0:$TM_SELECTED_TEXT}",
        "\\end{$1}"
      ]
    },
    "Align": {
      "prefix": "ali",
      "body": [
        "\\begin{align}",
        "\t${1:$TM_SELECTED_TEXT}",
        "\\end{align}"
      ]
    },
    "Fraction": {
       "prefix": "//",
       "body": "\\frac{${1:$TM_SELECTED_TEXT}}{$2}$0"
    },
    "Limit": {
        "prefix": "lim",
        "body": "\\lim_{${1|x,y,n|} \\to ${2|\\infty, |}}$0"
    },
    "Square Root": {
        "prefix": "sr",
        "body": "\\sqrt{${1:$TM_SELECTED_TEXT}}$0"
    },
    "Square": {
        "prefix": "^",
        "body": "^{${1:$TM_SELECTED_TEXT}}$0"
    },
    "Section": {
        "prefix": "sec",
        "body": [
          "\\section{${1:$TM_SELECTED_TEXT}}",
          "$0"
        ]
    },
    "Subsection": {
        "prefix": "ssec",
        "body": [
          "\\subsection{${1:$TM_SELECTED_TEXT}}",
          "$0"
        ]
    },
    "Subsubsection": {
        "prefix": "sssec",
        "body": [
          "\\subsubsection{${1:$TM_SELECTED_TEXT}}",
          "$0"
        ]
    },
    "Textnormal": {
        "prefix": "tn",
        "body": "\\textnormal{${1:$TM_SELECTED_TEXT}}$0"
    },
    "right arrow": {
        "prefix": "->",
        "body": "\\rightarrow$0"
    },
    "Right Arrow": {
        "prefix": "=>",
        "body": "\\Rightarrow$0"
    },
    "left arrow": {
        "prefix": "<-",
        "body": "\\leftarrow$0"
    },
    "Left Arrow": {
        "prefix": "<=",
        "body": "\\Leftarrow$0"
    },
    "Table": {
        "prefix": "table",
        "body": [
            "\\begin{table}[H]",
            "\\centering",
            "\\caption{$1}",
            "\\begin{tabular}{$2}",
            "$3",
            "\\end{tabular}",
            "\\end{table}"
        ]
    },
    "Bold": {
        "prefix": "bf",
        "body": "\\textbf{${1:$TM_SELECTED_TEXT}}$0"
    },
    "Italics": {
        "prefix": "it",
        "body": "\\textit{${1:$TM_SELECTED_TEXT}}$0"
    },
    "Paragraph": {
        "prefix": "par",
        "body": "\\paragraph{${1:$TM_SELECTED_TEXT}}$0"
    },
    "MLA": {
        "prefix": "MLA",
        "body": [
            "\\begin{document}",
            "\\pagestyle{myheadings}",
            "\\markright{\\hfill Modi}",
            "Shreeram Modi\\\\\\\\",
            "$1\\\\\\\\",
            "$2\\\\\\\\",
            "$3\\\\\\\\",
            "",
            "\\centerline{$4}",
            "",
            "\\setlength{\\parindent}{0.5in}",
            "",
            "\\nohyphens{",
            "",
            "$0",
            "",
            "}",
            "",
            "\\end{document}"
        ]
    }
}
